<%= form_for @animal do |f| %>


<%= f.label :name %>
<%= f.text_field :name %>
<br>
<%= f.label :species %>
<%= f.text_field :species %>
<br>
<%= f.label :breed %>
<%= f.text_field :breed %>
<br>
<%= f.label :arrival_time %>
<%= f.text_field :arrival_time %>
<br>
<%= f.label :departure_time %>
<%= f.text_field :departure_time %>
<br>
<%= f.label :age %>
<%= f.text_field :age %>
<br>
<%= f.submit %>
<%end%>